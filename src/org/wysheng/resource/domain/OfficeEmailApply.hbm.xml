<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="org.wysheng.resource.domain">
 <class name="OfficeEmailApply" table="OfficeEmailApply">
  <!--OfficeEmailApply（办公邮箱申请单） 
申请单ID -->
  <id column="applyID" name="applyID" type="integer"/>
  <!--空间-->
  <property generated="never" lazy="false" name="zone" type="integer"/>
  <!--办公地点 -->
  <property generated="never" lazy="false" name="officePlace" type="string"/>
  <!--使用者姓名 -->
  <property generated="never" lazy="false" name="userName" type="string"/>
  <!--姓名全拼 -->
  <property generated="never" lazy="false" name="fullName" type="string"/>
  <!--申请单ID -->
 </class>
</hibernate-mapping>
